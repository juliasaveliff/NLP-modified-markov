<!--ceo:replacemode=raw-->




    <figure style="max-width:100%;    " class="embedded-media embedded-image">
        
        <img src="https://snworksceo.imgix.net/dtc/4b2fef43-b97c-40d0-b5f1-2626419a3ae8.sized-1000x1000.jpg?w=1000" class="w-100" alt="" />                    <span class="embedded-credit">
